{
  "name": "@workhard/react-monorepo",
  "description": "Workhard Dev Kit for DAO Builders",
  "private": true,
  "version": "0.1.3",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "docs": "doctoc --title '**Table of content**' README.md",
    "clean": "lerna run clean",
    "build": "lerna run build",
    "test": "lerna run test",
    "lint": "eslint --ext js,ts,tsx .",
    "start": "lerna run start --scope=@workhard/frontend-v1 --stream"
  },
  "devDependencies": {
    "@nighttrax/eslint-config-tsx": "~8.0.0",
    "doctoc": "~2.0.0",
    "eslint": "~7.31.0",
    "eslint-plugin-import": "~2.23.0",
    "lerna": "^4.0.0",
    "react": "~17.0.0",
    "typescript": "~4.3.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
